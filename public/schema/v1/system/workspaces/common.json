{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Defines workspace types",
  "definitions": {
    "WorkspaceStatus": {
      "description": "Defines the workspace status",
      "type": "string",
      "enum": [
        "ENABLED",
        "DISABLED"
      ],
      "additionalItems": false
    },
    "TokenAlgorithm": {
      "type": "string",
      "description": "Algorithm type used to encrypt tokens",
      "enum": [
        "HMD5",
        "HS224",
        "HS256",
        "HS384",
        "HS512",
        "RS256",
        "RS384",
        "RS512",
        "ES256",
        "ES384",
        "ES512"
      ],
      "additionalItems": false
    }
  }
}